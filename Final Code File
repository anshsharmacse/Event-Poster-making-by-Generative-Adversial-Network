      "cell_type": "markdown",
      "source": [
        "# Generating 20 Posters of Various **Events**"
      ],
      "metadata": {
        "id": "gh22f2xryI0N"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "import os\n",
        "import torch\n",
        "from torchvision.utils import save_image\n",
        "\n",
        "# Directories for saving generated images\n",
        "generated_dir = './generated_posters'\n",
        "os.makedirs(generated_dir, exist_ok=True)\n",
        "\n",
        "# Load the trained Generator\n",
        "latent_dim = 100\n",
        "label_dim = 10\n",
        "num_images = 20\n",
        "generator = Generator(latent_dim, label_dim).cpu()\n",
        "torch.save(generator.state_dict(), 'generator_checkpoint.pth')\n",
        "generator.load_state_dict(torch.load('generator_checkpoint.pth', map_location='cpu'))\n",
        "generator.eval()\n",
        "\n",
        "# Generate and save synthetic images\n",
        "for i in range(num_images):\n",
        "    with torch.no_grad():\n",
        "        z = torch.randn(1, latent_dim)\n",
        "        label = torch.zeros(1, label_dim)\n",
        "        label[0, 0] = 1  # Example: Setting label for category \"Music\"\n",
        "        fake_img = generator(z, label)\n",
        "        fake_img = fake_img.squeeze(0)\n",
        "        save_image(fake_img, os.path.join(generated_dir, f'poster_{i+1}.png'), normalize=True)\n",
        "\n",
        "print(f\"Generated {num_images} images in the directory: {generated_dir}\")\n"
      ],
      "metadata": {
        "id": "xdkl5zkjqcqt",
        "outputId": "121964b0-13ba-4d69-ff1c-3bc0b4b085b2",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 0
        }
      },

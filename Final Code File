    {
      "cell_type": "markdown",
      "source": [
        "# Training Generatial Adversial Network with 100 **epochs**"
      ],
      "metadata": {
        "id": "-g6yw1uvx76x"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "from torch.utils.data import DataLoader, Dataset\n",
        "\n",
        "# Example metadata (replace with actual data)\n",
        "metadata = [\"music\", \"art\", \"sports\", \"conference\", \"party\"]\n",
        "label_map = {label: idx for idx, label in enumerate(metadata)}  # Map labels to integers\n",
        "\n",
        "# Label encoding (dummy example)\n",
        "labels = [label_map[\"music\"]] * 50 + [label_map[\"art\"]] * 50  # Repeat for example purposes\n",
        "\n",
        "# Convert data and labels into tensors\n",
        "import torch\n",
        "data_tensor = torch.tensor(data, dtype=torch.float32)\n",
        "labels_tensor = torch.tensor(labels, dtype=torch.long)\n",
        "\n",
        "class PosterDataset(Dataset):\n",
        "    def __init__(self, images, labels):\n",
        "        self.images = images\n",
        "        self.labels = labels\n",
        "\n",
        "    def __len__(self):\n",
        "        return len(self.images)\n",
        "\n",
        "    def __getitem__(self, idx):\n",
        "        # Convert label to one-hot\n",
        "        label = torch.zeros(len(metadata))\n",
        "        label[self.labels[idx]] = 1\n",
        "        return self.images[idx], label\n",
        "\n",
        "# Create data loader\n",
        "dataset = PosterDataset(data_tensor, labels_tensor)\n",
        "dataloader = DataLoader(dataset, batch_size=32, shuffle=True)\n",
        "\n",
        "# Hyperparameters\n",
        "latent_dim = 100  # Latent dimension for noise vector\n",
        "label_dim = len(metadata)  # Number of categories\n",
        "epochs = 100\n",
        "lr = 0.0002  # Learning rate\n",
        "\n",
        "# Initialize models\n",
        "generator = Generator(latent_dim, label_dim).cpu()\n",
        "discriminator = Discriminator(label_dim).cpu()\n",
        "\n",
        "# Loss function and optimizers\n",
        "criterion = nn.BCELoss()\n",
        "optim_G = optim.Adam(generator.parameters(), lr=lr, betas=(0.5, 0.999))\n",
        "optim_D = optim.Adam(discriminator.parameters(), lr=lr, betas=(0.5, 0.999))\n",
        "\n",
        "# Training loop\n",
        "for epoch in range(epochs):\n",
        "    for real_imgs, labels in dataloader:\n",
        "        # Move to CPU\n",
        "        real_imgs, labels = real_imgs.cpu(), labels.cpu()\n",
        "\n",
        "        # Create labels for real and fake images\n",
        "        real_labels = torch.ones(real_imgs.size(0), 1).cpu()  # Real labels\n",
        "        fake_labels = torch.zeros(real_imgs.size(0), 1).cpu()  # Fake labels\n",
        "\n",
        "        # Train the discriminator\n",
        "        optim_D.zero_grad()\n",
        "        output_real = discriminator(real_imgs, labels)\n",
        "        d_loss_real = criterion(output_real, real_labels)\n",
        "\n",
        "        z = torch.randn(real_imgs.size(0), latent_dim).cpu()  # Latent vector\n",
        "        fake_imgs = generator(z, labels)\n",
        "        output_fake = discriminator(fake_imgs.detach(), labels)\n",
        "        d_loss_fake = criterion(output_fake, fake_labels)\n",
        "\n",
        "        d_loss = d_loss_real + d_loss_fake\n",
        "        d_loss.backward()\n",
        "        optim_D.step()\n",
        "\n",
        "        # Train the generator\n",
        "        optim_G.zero_grad()\n",
        "        output_fake = discriminator(fake_imgs, labels)\n",
        "        g_loss = criterion(output_fake, real_labels)\n",
        "        g_loss.backward()\n",
        "        optim_G.step()\n",
        "\n",
        "    print(f\"Epoch [{epoch+1}/{epochs}] | D Loss: {d_loss.item():.4f} | G Loss: {g_loss.item():.4f}\")\n"
      ],
      "metadata": {
        "id": "VL4lgXKNdr26",
        "outputId": "2f8aa454-0f00-4cc2-cee1-fd1247103e8f",
        "colab": {

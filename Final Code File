        "import requests\n",
        "from bs4 import BeautifulSoup\n",
        "import os\n",
        "\n",
        "# Create a folder for storing the posters\n",
        "os.makedirs(\"event_posters\", exist_ok=True)\n",
        "\n",
        "url = 'https://venngage.com/templates/posters'  # Example URL\n",
        "response = requests.get(url)\n",
        "soup = BeautifulSoup(response.content, 'html.parser')\n",
        "\n",
        "# Find poster images and store them, ensuring they are valid image formats\n",
        "posters = soup.find_all('img')  # Find all images\n",
        "for idx, poster in enumerate(posters):\n",
        "    img_url = poster['src']\n",
        "    try:\n",
        "        # Check if the image URL is valid and has a recognized image extension\n",
        "        if img_url and any(img_url.lower().endswith(ext) for ext in ['.jpg', '.jpeg', '.png', '.gif', '.bmp']):\n",
        "            img_data = requests.get(img_url).content\n",
        "            with open(f\"event_posters/poster_{idx}.jpg\", 'wb') as f:\n",
        "                f.write(img_data)\n",
        "        else:\n",
        "            print(f\"Skipping invalid image URL: {img_url}\")\n",
        "    except Exception as e:\n",
        "        print(f\"Error saving image {idx}: {e}\")"
      ],
